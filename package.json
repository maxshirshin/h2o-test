{
  "private": true,
  "name": "remix-oxygen-template",
  "main": "./worker-dist/index.js",
  "description": "A demo of Remix running on the Oxygen platform.",
  "license": "MIT",
  "scripts": {
    "cleanup": "rimraf .cache ./build ./public/build ./worker-dist",
    "build:tailwind": "tailwindcss --minify -o app/styles/tailwind.css",
    "build:remix": "remix build",
    "build:worker": "NODE_ENV=production ts-node ./build.ts",
    "build:patch": "sed 's/caches.default/caches[\"default\"]/g' ./worker-dist/index.js > ./worker-dist/index-patched.js && mv ./worker-dist/index-patched.js ./worker-dist/index.js",
    "build": "yarn run build:tailwind && yarn run build:remix && yarn run build:worker && yarn run build:patch",
    "dev:tailwind": "tailwindcss -o app/styles/tailwind.css --watch",
    "dev:miniflare": "yarn exec oxygen-preview",
    "dev:remix": "remix watch",
    "dev": "NODE_ENV=development concurrently \"npm:dev:*\"",
    "postinstall": "remix setup cloudflare",
    "prebuild": "yarn run cleanup",
    "start": "yarn exec oxygen-preview"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "@remix-run/cloudflare-workers": "^1.0.6",
    "@remix-run/react": "^1.0.6",
    "@shopify/mini-oxygen": "^0.0.5",
    "classnames": "^2.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.2",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@cloudflare/wrangler": "^1.19.4",
    "@remix-run/dev": "^1.0.6",
    "@shopify/oxygen-workers-types": "^1.0.3",
    "@tailwindcss/typography": "^0.4.1",
    "@testing-library/cypress": "^8.0.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "concurrently": "^6.3.0",
    "cypress": "^8.7.0",
    "daisyui": "^1.16.2",
    "esbuild": "^0.13.12",
    "miniflare": "^1.4.1",
    "patch-package": "^6.4.7",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
